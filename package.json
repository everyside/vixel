    "nyc": "^13.1.0",
    "terser": "^3.11.0",
    "terser-webpack-plugin": "^1.1.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "yargs": "^12.0.5"
  },
  "nyc": {
    "sourceMap": false,
